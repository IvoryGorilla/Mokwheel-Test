        "item_w9jTqk": {
          "type": "item",
          "settings": {
            "title": "Mission",
            "content": "<p>Be the leading force of revolutionizing, intelligent transportation products to make the world a healthier, more enjoyable, and more efficient place.</p>"
          }
        },
        "item_fABqNH": {
          "type": "item",
          "settings": {
            "title": "Vision",
            "content": "<p>Mokwheel aims to bring value to its customers through innovation, with ebikes that hold true value.</p>"
          }
        }
