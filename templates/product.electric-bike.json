        "73e11739-b582-4da7-b3dd-05909fe9baab": {
          "type": "alp_popup_slide",
          "settings": {
            "title": "Size & Fit",
            "rich_content_1": "<div class=\"alp_su_one\">\n  <p>Asphalt ST comes in 1 size to suit most adults</p>\n  <p><b>Large : </b>5'6\" - 6'6\"</p>\n<p><b>Normal: </b>5'2\" - 6'2\"</p>\n</div>",
